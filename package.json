{
  "name": "javascript-development-environment",
  "version": "1.0.0",
  "description": "Web App Starter",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch lint:css lint:html",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "localtunnel": "ngrok http 3000",
     "localtunnell": "lt --port 3000 --subdomain 'leehbi'",
    "share": "npm-run-all --parallel open:src localtunnel",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint:html": "htmllint src/**/*.html"
  },
  "author": "Lee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/leewebby/webstarter.git"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.0",
    "babel-register": "6.26.0",
    "chai": "4.1.2",
    "chalk": "2.3.0",
    "cheerio": "1.0.0-rc.2",
    "compression": "1.7.1",
    "cross-env": "5.0.5",
    "css-loader": "0.28.7",
    "eslint": "4.8.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-watch": "3.1.2",
    "express": "4.16.2",
    "extract-text-webpack-plugin": "3.0.1",
    "html-webpack-plugin": "2.30.1",
    "htmllint": "^0.6.0",
    "htmllint-cli": "0.0.6",
    "jsdom": "9.11.0",
    "json-schema-faker": "0.4.0",
    "json-server": "0.12.0",
    "localtunnel": "1.8.3",
    "mocha": "4.0.1",
    "nock": "9.0.22",
    "npm-run-all": "4.1.1",
    "nsp": "2.8.1",
    "numeral": "2.0.6",
    "open": "0.0.5",
    "rimraf": "2.6.2",
    "style-loader": "0.19.0",
    "stylelint": "^7.13.0",
    "stylelint-config-standard": "^17.0.0",
    "webpack": "3.6.0",
    "webpack-dev-middleware": "1.12.0",
    "webpack-hot-middleware": "2.19.1",
    "webpack-md5-hash": "0.0.5"
  }
}
